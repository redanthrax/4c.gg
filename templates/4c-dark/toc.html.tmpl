{{>partials/class.header}}
{{#_enableSearch}}<div class="hide-when-search">{{/_enableSearch}}
{{^_disableNavbar}}
{{>partials/navbar}}
{{/_disableNavbar}}
{{#_enableSearch}}</div>{{/_enableSearch}}

<div role="main" class="container-fluid hide-when-search">
  {{^_disableBreadcrumb}}
  {{>partials/breadcrumb}}
  {{/_disableBreadcrumb}}
  
  <div class="row">
    <div class="col-12">
      <article class="content wrap" id="_content" data-uid="{{uid}}">
        <h1>{{title}}</h1>
        <div class="toc">
          {{{conceptual}}}
        </div>
      </article>
    </div>
  </div>
</div>

{{^_disableFooter}}
{{>partials/footer}}
{{/_disableFooter}}

{{#_enableSearch}}
<div id="mask" class="mask"></div>
<div id="overlay" class="overlay">
  <div id="search-results" class="search-results">
    <div class="search-list"></div>
    <div class="sr-items"></div>
  </div>
</div>
{{/_enableSearch}}

{{>partials/scripts}}
